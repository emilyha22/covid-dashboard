{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "serve": "firebase emulators:start --only functions,firestore",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test-without-emulator": "mocha --reporter spec",
    "test": "firebase emulators:exec \"mocha --reporter spec\""
  },
  "engines": {
    "node": "12"
  },
  "main": "index.js",
  "dependencies": {
    "chai": "^4.3.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.5",
    "csv": "^5.3.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fast-crc32c": "^2.0.0",
    "firebase-admin": "^9.5.0",
    "firebase-functions": "^3.13.1",
    "got": "^11.8.1"
  },
  "devDependencies": {
    "firebase-functions-test": "^0.2.3",
    "mocha": "^8.3.0",
    "sinon": "^9.2.4"
  },
  "private": true
}
